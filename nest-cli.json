{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api",
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "person": {
      "type": "application",
      "root": "apps/person",
      "entryFile": "main",
      "sourceRoot": "apps/person/src",
      "compilerOptions": {
        "tsConfigPath": "apps/person/tsconfig.app.json"
      }
    },
    "film": {
      "type": "application",
      "root": "apps/film",
      "entryFile": "main",
      "sourceRoot": "apps/film/src",
      "compilerOptions": {
        "tsConfigPath": "apps/film/tsconfig.app.json"
      }
    },
    "country": {
      "type": "application",
      "root": "apps/country",
      "entryFile": "main",
      "sourceRoot": "apps/country/src",
      "compilerOptions": {
        "tsConfigPath": "apps/country/tsconfig.app.json"
      }
    },
    "genre": {
      "type": "application",
      "root": "apps/genre",
      "entryFile": "main",
      "sourceRoot": "apps/genre/src",
      "compilerOptions": {
        "tsConfigPath": "apps/genre/tsconfig.app.json"
      }
    },
    "occupation": {
      "type": "application",
      "root": "apps/occupation",
      "entryFile": "main",
      "sourceRoot": "apps/occupation/src",
      "compilerOptions": {
        "tsConfigPath": "apps/occupation/tsconfig.app.json"
      }
    },
    "other": {
      "type": "application",
      "root": "apps/other",
      "entryFile": "main",
      "sourceRoot": "apps/other/src",
      "compilerOptions": {
        "tsConfigPath": "apps/other/tsconfig.app.json"
      }
    },
    "files": {
      "type": "application",
      "root": "apps/files",
      "entryFile": "main",
      "sourceRoot": "apps/files/src",
      "compilerOptions": {
        "tsConfigPath": "apps/files/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    }
  }
}